typedef signed long int64_t;
typedef signed long uint64_t;

kernel void sum(global long *starts, global long *ends, global long *results) {
    int id = get_global_id(0);
    int64_t start = starts[id];
    int64_t end = ends[id];
    int64_t result = 0;
    for (int i = start; i < end; i++) {
        result += i;
    }
    results[id] = result;
}
